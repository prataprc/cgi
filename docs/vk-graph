VkMemoryHeap, VkMemoryType, VkRect2D, VkOffset2D, VkExtent2D, VkExtent3D

timeout - uint64
layers, extensions - String
qf - queue-family
qc - queue-count
qp - queue-priority
qi - queue-index
ext2 - VkExtent2D
ext3 - VkExtent3D
iCr - VkImageCreateFlagBits
iUsg - VkImageUsageFlagBits
iTyp - VkImageType
iTil - VkImageTiling
layout - VkImageLayout
image-sub-range - VkImageSubresourceRange
image-sub - VkImageSubresource
lvls - mip-levels
lyrs - array-layers
sub-layout - VkSubresourceLayout
smpl - VkSampleCountFlagBits
cs - VkColorSpaceKHR
ca - VkCompositeAlphaFlagBitsKHR
shrg - VkSharingMode
vCr - VkImageViewCreateFlagBits, VkBufferViewCreateFlagBits
vTyp - VkBufferViewTyp, VkImageViewType
swizzling - VkComponentMapping
bCr - VkBufferCreateFlagBits
bUsg - VkBufferUsageFlagBits
present - VkPresentModeKHR
trfm - VkSurfaceTransformFlagBitsKHR

