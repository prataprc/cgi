                                                                            float gl_PointSize;                };
                                                                            float gl_ClipDistance[];           out int gl_PrimitiveID;        in int gl_PrimitiveID;
                                                                            float gl_CullDistance[];           out int gl_Layer;              in int gl_Layer;
                                                                        };                                     out int gl_ViewportIndex;      in int gl_ViewportIndex;
                                                                                                                                              in vec4 gl_FragCoord;
